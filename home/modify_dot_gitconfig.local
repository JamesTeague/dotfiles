{{- /* chezmoi:modify-template */ -}}
{{- $helper := osxkeychain -}} 
{{- if eq .chezmoi.os "linux" -}}
{{-    $helper = cache -}}
{{- end -}}
{{- $scriptPath := printf "%s/scripts/generate-gpg-key.sh" .chezmoi.sourceDir -}}
{{- $keyValue := output $scriptPath -}}
{{- .chezmoi.stdin | replaceAllRegex "data.name" .name | replaceAllRegex "data.email" .email | replaceAllRegex "data.helper" $helper -}}
